{
    "name": "jenkins-extension-pack",
    "displayName": "Jenkins Extension Pack",
    "publisher": "DontShaveTheYak",
    "description": "Make Jenkins development quick and easy.",
    "homepage": "https://github.com/DontShaveTheYak/jenkins-extension-pack",
    "repository": {
        "type": "git",
        "url": "https://github.com/DontShaveTheYak/jenkins-extension-pack.git"
    },
    "version": "0.0.0",
    "keywords": [
        "jenkins",
        "groovy",
        "vscode",
        "extension"
    ],
    "engines": {
        "vscode": "^1.58.0"
    },
    "categories": [
        "Extension Packs"
    ],
    "extensionPack": [
        "Maarti.jenkins-doc",
        "NicolasVuillamy.vscode-groovy-lint",
        "DontShaveTheYak.groovy-guru"
    ],
    "scripts": {
        "package": "vsce package --no-git-tag-version",
        "publish": "vsce publish --no-git-tag-version --packagePath"
    },
    "icon": "icon.png",
    "devDependencies": {
        "vsce": "^1.95.1"
    }
}
